    "keywords": ["container", "properties", "property"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@cego.dk"
        }
    ],
    "require": {
        "doctrine/inflector": "^1.1"
    },
    "autoload": {
        "psr-4": {
